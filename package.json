{
  "name": "toptal-restaurants",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "test": "react-scripts test --env=node",
    "dev": "concurrently npm:server npm:client",
    "server": "nodemon src/0.bin/server.ts",
    "client": "react-scripts start",
    "build:client": "react-scripts build",
    "start": "ts-node --transpile-only src/0.bin/server.ts",
    "script:codegen": "graphql-codegen --config codegen.yml",
    "lint": "npx eslint src/*",
    "typecheck": "tsc",
    "prepare": "husky install",
    "script:database:migrate": "prisma migrate dev --schema src/3.infrastructure/database/prisma/schema.prisma"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/**/*.{ts,tsx}": "eslint --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://git.toptal.com/screening/Eric-Quanz-2.git"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@prisma/client": "^3.1.1",
    "apollo-server": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "uuid": "^8.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "prisma": "^3.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
